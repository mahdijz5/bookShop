{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/reservation/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/reservation/tsconfig.app.json"
  },
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "micro-nestjs": {
      "type": "application",
      "root": "apps/micro-nestjs",
      "entryFile": "main",
      "sourceRoot": "apps/micro-nestjs/src",
      "compilerOptions": {
        "tsConfigPath": "apps/micro-nestjs/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "ipg": {
      "type": "application",
      "root": "apps/ipg",
      "entryFile": "main",
      "sourceRoot": "apps/ipg/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ipg/tsconfig.app.json"
      }
    },
    "core": {
      "type": "application",
      "root": "apps/core",
      "entryFile": "main",
      "sourceRoot": "apps/core/src",
      "compilerOptions": {
        "tsConfigPath": "apps/core/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/reservation"
}